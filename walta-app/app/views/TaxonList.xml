<Alloy>
	<ListView id="content" class="container" accessibilityHidden="true" defaultItemTemplate="taxon" onItemclick="clickItem">
		<Templates>
			<ItemTemplate name="taxon" accessibilityHidden="true" >
				<Label bindId="title" id="taxon" class="speciesItem"/>
			</ItemTemplate>
			<ItemTemplate name="genusOrSpeciesTaxon" accessibilityHidden="true" >
				<Label bindId="title" id="genusOrSpeciesTaxon" class="speciesItem"/>
			</ItemTemplate>
		</Templates>
	</ListView>
</Alloy>